# baekjoon 10809번 알파벳찾기




#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <string.h>

int main(void)
{
	int alpha[26], i, j;			       # alpha배열은 알파벳 순서 ex) a = alpha[0], b = alpha[1]
	char S[100];                                   # 입력받을 문자열

	scanf("%s", S);

	for (i = 0; i < 26; i++)
        alpha[i] = -1;

    
    for (i = 0; i < strlen(S); i++)
        {
            j = S[i] - 'a';
            if(alpha[j]==-1) alpha[j] = i;       
        }
   

    for (i = 0; i < 26; i++)
        printf("%d ", alpha[i]);
      
    return 0;
}





